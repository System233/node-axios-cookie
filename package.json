{
  "name": "axios-cookie",
  "version": "0.0.1",
  "description": "Axios CookieJar Support",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "type": "module",
  "exports": {
    ".":{
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./lib/index.ts"
    },
    "./adapter":{
      "import":"./dist/esm/adapter.js",
      "require":"./dist/cjs/adapters.js",
      "types":"./lib/adapters.ts"
    },
    "./adapters/*":{
      "import":"./dist/esm/adapters/*",
      "require":"./dist/cjs/adapters/*",
      "types":"./lib/adapters/*"
    }
  },
  "repository": "https://github.com/System233/node-axios-cookie.git",
  "author": "System233 <cdkeylzh@outlook.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc&&tsc --project tsconfig.cjs.json",
    "docs": "typedoc",
    "test": "node --test&&cross-env USE_TOUGH_COOKIE=1 node --test"
  },
  "optionalDependencies": {
    "tough-cookie": "^4.1.3"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cookiejar": "^2.1.5",
    "@types/node": "^20.12.7",
    "@types/tough-cookie": "^4.0.5",
    "axios": "^1.6.8",
    "axios-mock-adapter": "^1.22.0",
    "cross-env": "^7.0.3",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.4.5"
  }
}
