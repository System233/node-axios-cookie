{
  "compilerOptions": {
    "outDir": "dist/esm",
    "target": "ESNext",
    "module": "NodeNext",
    "declaration": true,
    // "sourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "NodeNext",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "typedocOptions": {
    "name":"Axios-Cookies",
    "entryPoints": ["lib/**/*.ts"],
    "out": "docs",
    "plugin": ["typedoc-plugin-markdown"]
  }
}
